{
    "name": "ASDF install",
    "id": "asdf",
    "version": "1.0.2",
    "description": "Install anything with ASDF from `.tool-versions`",
    "options": {
        "plugins": {
            "type": "string",
            "proposals": [
                "erlang,elixir",
                "elixir"
            ],
            "description": "A comma-separated list of plugins to install. Must match versions in `.tool-versions`."
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
